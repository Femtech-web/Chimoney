{
  "name": "chimoney",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "lint:staged": "lint-staged",
    "pre-push": "npm run pre-push:test",
    "pre-push:test": "prepush-if-changed"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@mui/material-nextjs": "^5.15.9",
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "firebase": "^10.8.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,html,md}": "prettier --write"
  },
  "prepush-if-changed": {
    "src/**/*.{js,jsx,ts,tsx}": "echo 'Files changed on App. Launching tests.' && npm run test"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prepush-if-changed": "^1.0.8",
    "tsc-files": "^1.1.4",
    "typescript": "^5"
  }
}
